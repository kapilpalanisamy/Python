    "#check if the array is sorted \n",
    "arr=[1,2,2,3,3,4]\n",
    "def sort_arr(arr):\n",
    "    for i in range(1,len(arr)):\n",
    "        if arr[i]>=arr[i-1]:\n",
    "            pass\n",
    "        else:\n",
    "            return False\n",
    "    return True\n",
    "a=sort_arr(arr)\n",
    "print(a)\n",
    "#time O(n)"
   ]
