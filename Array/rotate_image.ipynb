   "source": [
    "#brute force solution\n",
    "#Input: matrix = [[1,2,3],[4,5,6],[7,8,9]]\n",
    "#Output: [[7,4,1],[8,5,2],[9,6,3]]\n",
    "matrix = [[1,2,3],[4,5,6],[7,8,9]]\n",
    "n=len(matrix)\n",
    "ans=[[0]*n for _ in range(n)]\n",
    "print(ans)\n",
    "for i in range(n):\n",
    "    for j in range(n):\n",
    "        ans[j][n-1-i]=matrix[i][j]\n",
    "print(ans)"
   ]
