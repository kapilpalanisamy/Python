   "source": [
    "s = \"abcabcbb\"\n",
    "max1 = 0\n",
    "start=0\n",
    "\n",
    "seen={}\n",
    "for i,ch in enumerate(s):\n",
    "    if ch in seen and seen[ch]>=start:\n",
    "        start=seen[ch]+1\n",
    "    seen[ch]=i\n",
    "    length=i-start+1\n",
    "    max1=max(max1,length)\n",
    "print(max1)"
   ]
